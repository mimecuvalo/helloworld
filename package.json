{
  "name": "hello-world-blog",
  "version": "0.8.7",
  "description": "hello, world. a federated blog with feed reader.",
  "author": "Mime CÌŒuvalo <mimecuvalo@gmail.com>",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/mimecuvalo/helloworld.git"
  },
  "keywords": [
    "cra",
    "create-react-app",
    "hello-world",
    "federated",
    "react",
    "swat0"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=15.14.0",
    "npm": ">=7.7.6"
  },
  "bugs": {
    "url": "https://github.com/mimecuvalo/helloworld/issues"
  },
  "scripts": {
    "predev": "yarn i18n:compile",
    "dev": "open http://localhost:3000 && NODE_OPTIONS='--inspect' next dev",
    "prebuild": "yarn i18n:compile",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint --fix .",
    "i18n:extract": "formatjs extract 'pages/**/*.ts*' 'components/**/*.ts*' --out-file i18n/en.json --additional-component-names F --id-interpolation-pattern '[md5:contenthash:hex:10]' --format smartling",
    "i18n:compile": "formatjs compile-folder i18n i18n-compiled-lang --format smartling",
    "alex": "alex .",
    "analyze": "source-map-explorer .next/static/**/*.js",
    "codegen": "graphql-codegen",
    "config": "node bin/config.js",
    "docker": "npm install && docker-compose up -d && npx sequelize db:migrate && npx sequelize db:seed:all",
    "prepare": "husky install",
    "prettier:all": "prettier --check .",
    "prettier:fix": "prettier --write",
    "prettier:fix:all": "prettier --write .",
    "postinstall": "prisma generate",
    "ts-script": "ts-node-script -r tsconfig-paths/register",
    "build-storybook": "build-storybook -s public",
    "storybook": "start-storybook -p 9001 -c .storybook --ci --quiet",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "3.5.8",
    "@auth0/nextjs-auth0": "1.7.0",
    "@emotion/react": "11.7.1",
    "@emotion/server": "11.4.0",
    "@emotion/styled": "11.6.0",
    "@mui/icons-material": "5.0.3",
    "@mui/material": "5.0.3",
    "@prisma/client": "3.9.1",
    "@sentry/nextjs": "6.17.3",
    "apollo-server-micro": "3.6.2",
    "aws-sdk": "2.1130.0",
    "babel-plugin-formatjs": "10.3.5",
    "body-parser": "1.19.1",
    "cheerio": "1.0.0-rc.3",
    "circular-dependency-plugin": "5.2.2",
    "classnames": "2.3.1",
    "cookie-parser": "1.4.6",
    "dataloader": "2.0.0",
    "deepmerge": "4.2.2",
    "feedparser": "2.2.10",
    "graphql": "16.3.0",
    "graphql-resolvers": "0.4.2",
    "graphql-tools": "8.2.0",
    "hello-world-editor": "12.0.3",
    "js-cookie": "3.0.1",
    "lodash": "4.17.21",
    "magic-signatures": "2.0.0",
    "md5.js": "1.3.5",
    "micro": "9.3.4",
    "mime": "2.5.2",
    "nanoid": "3.1.22",
    "next": "12.2.0",
    "node-fetch": "2.6.7",
    "node-forge": "0.10.0",
    "node-schedule": "2.0.0",
    "prisma": "3.9.1",
    "pubsubhubbub": "1.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "5.24.4",
    "react-spring": "9.4.2",
    "sanitize-html": "2.3.3",
    "sanitize.css": "13.0.0",
    "sharp": "0.30.6",
    "styled-components": "5.3.5",
    "swipe-listener": "1.3.0",
    "uuid": "8.3.2",
    "web-vitals": "2.1.0",
    "workbox-background-sync": "6.4.2",
    "workbox-broadcast-update": "6.4.2",
    "workbox-cacheable-response": "6.4.2",
    "workbox-core": "6.4.2",
    "workbox-expiration": "6.4.2",
    "workbox-google-analytics": "6.4.2",
    "workbox-navigation-preload": "6.4.2",
    "workbox-precaching": "6.4.2",
    "workbox-range-requests": "6.4.2",
    "workbox-routing": "6.4.2",
    "workbox-strategies": "6.4.2",
    "workbox-streams": "6.4.2"
  },
  "devDependencies": {
    "@formatjs/cli": "4.8.1",
    "@graphql-codegen/add": "3.1.1",
    "@graphql-codegen/cli": "2.4.0",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-operations": "2.4.1",
    "@graphql-codegen/typescript-resolvers": "2.6.5",
    "@storybook/addon-essentials": "6.0.26",
    "@storybook/addon-links": "6.0.26",
    "@storybook/react": "6.0.26",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/user-event": "13.2.1",
    "@types/cheerio": "0.22.31",
    "@types/graphql-resolvers": "0.3.1",
    "@types/js-cookie": "3.0.2",
    "@types/lodash": "4.14.178",
    "@types/mime": "2.0.3",
    "@types/node": "18.0.0",
    "@types/react": "18.0.21",
    "@types/sanitize-html": "2.6.2",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "alex": "10.0.0",
    "axe-core": "4.3.5",
    "babel-jest": "27.4.5",
    "cross-fetch": "3.1.5",
    "eslint": "8.7.0",
    "eslint-config-next": "12.0.9",
    "eslint-plugin-formatjs": "2.17.4",
    "eslint-plugin-testing-library": "5.0.1",
    "husky": "6.0.0",
    "import-sort-style-eslint": "6.0.0",
    "jest": "27.4.5",
    "lint-staged": "10.5.4",
    "prettier": "2.5.1",
    "prettier-plugin-import-sort": "0.0.7",
    "source-map-explorer": "2.5.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.5"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "**/*/(schema|resolvers)/*.ts": [
      "yarn codegen"
    ]
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "eslint"
    }
  },
  "browserslist": "last 2 versions",
  "sideEffects": false
}
