{
  "name": "helloworld",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-link": "1.2.6",
    "apollo-link-error": "1.1.5",
    "apollo-link-http": "1.5.9",
    "classnames": "2.2.6",
    "cra-all-the-things": "0.5.1",
    "graphql-resolvers": "0.3.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "scripts": {
    "start": "npm-run-all --parallel serve:dev styleguide",
    "serve:dev": "react-scripts serve-dev",
    "serve:prod": "react-scripts serve-prod",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/main.*",
    "flow": "flow",
    "postinstall": "flow-typed install",
    "build-storybook": "build-storybook -s public",
    "styleguide": "start-storybook -p 9001 -c .storybook --ci --quiet"
  },
  "devDependencies": {
    "@storybook/addon-actions": "4.0.7",
    "@storybook/addon-links": "4.0.7",
    "@storybook/addons": "4.0.7",
    "@storybook/react": "4.0.7",
    "axe-core": "3.1.2",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "flow-bin": "0.86.0",
    "flow-typed": "2.5.1",
    "husky": "1.1.2",
    "lint-staged": "8.1.0",
    "memory-fs": "0.4.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.14.3",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-test-renderer": "16.7.0",
    "require-from-string": "2.0.2",
    "source-map-explorer": "1.6.0",
    "webpack-node-externals": "1.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "(client|server|shared)/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
