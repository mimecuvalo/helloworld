{
  "name": "hello-world-blog",
  "version": "0.8.2",
  "description": "hello, world. a federated blog with feed reader.",
  "repository": "mimecuvalo/helloworld",
  "license": "MIT",
  "engines": {
    "node": ">=8.10"
  },
  "author": "Mime CÌŒuvalo <mimecuvalo@gmail.com>",
  "bugs": {
    "url": "https://github.com/mimecuvalo/helloworld/issues"
  },
  "keywords": [
    "cra",
    "create-react-app",
    "hello-world",
    "federated",
    "react",
    "swat0"
  ],
  "dependencies": {
    "cra-all-the-things": "0.8.6",
    "dataloader": "1.4.0",
    "hello-world-editor": "0.8.3",
    "is-mobile": "2.0.1",
    "magic-signatures": "2.0.0",
    "nanoid": "2.0.1",
    "react": "16.8.4",
    "react-document-title": "2.0.3",
    "react-dom": "16.8.4",
    "react-jss": "8.6.1",
    "sharp": "0.21.3",
    "social-butterfly": "0.8.3",
    "swipe-listener": "1.0.7",
    "uuid": "3.3.2"
  },
  "scripts": {
    "start": "npm-run-all --parallel serve:dev styleguide",
    "serve:dev": "react-scripts serve-dev",
    "serve:prod": "react-scripts serve-prod",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/main.*",
    "flow": "flow",
    "postinstall": "if test \"$NODE_ENV\" = \"development\" ; then flow-typed install; fi",
    "build-storybook": "build-storybook -s public",
    "styleguide": "start-storybook -p 9001 -c .storybook --ci --quiet"
  },
  "devDependencies": {
    "@storybook/addon-actions": "5.1.0-beta.0",
    "@storybook/addon-links": "5.1.0-beta.0",
    "@storybook/addons": "5.1.0-beta.0",
    "@storybook/react": "5.1.0-beta.0",
    "axe-core": "3.2.2",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "flightplan": "0.6.19",
    "flow-bin": "0.98.1",
    "flow-typed": "2.5.1",
    "husky": "1.1.2",
    "lerna": "^3.14.1",
    "lint-staged": "8.1.0",
    "memory-fs": "0.4.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.14.3",
    "react-test-renderer": "16.7.0",
    "require-from-string": "2.0.2",
    "source-map-explorer": "1.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "(client|server|shared)/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
