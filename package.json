{
  "name": "helloworld",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-link-batch-http": "1.2.8",
    "cheerio": "1.0.0-rc.2",
    "cra-all-the-things": "0.8.2",
    "dataloader": "1.4.0",
    "draft-convert": "2.1.4",
    "draft-extend": "1.6.1",
    "draft-js": "0.10.5",
    "draft-js-alignment-plugin": "2.0.5",
    "draft-js-anchor-plugin": "2.0.2",
    "draft-js-divider-plugin": "0.2.1",
    "draft-js-drag-n-drop-plugin": "2.0.3",
    "draft-js-emoji-plugin": "2.1.1",
    "draft-js-focus-plugin": "2.2.0",
    "draft-js-hashtag-plugin": "2.0.3",
    "draft-js-inline-toolbar-plugin": "3.0.0",
    "draft-js-linkify-plugin": "2.0.1",
    "draft-js-mention-plugin": "3.1.3",
    "draft-js-plugins-editor": "2.1.1",
    "draft-js-side-toolbar-plugin": "3.0.1",
    "feedparser": "2.2.9",
    "mime": "2.4.2",
    "multer": "1.4.1",
    "nanoid": "2.0.1",
    "node-schedule": "1.3.1",
    "nodemailer": "6.1.1",
    "notistack": "0.4.2",
    "pubsubhubbub": "1.0.1",
    "react": "^16.7.0",
    "react-codemirror": "1.0.0",
    "react-document-title": "2.0.3",
    "react-dom": "^16.7.0",
    "react-jss": "8.6.1",
    "sanitize-html": "1.20.0",
    "sharp": "0.21.3",
    "uuid": "3.3.2"
  },
  "scripts": {
    "start": "npm-run-all --parallel serve:dev styleguide",
    "serve:dev": "react-scripts serve-dev",
    "serve:prod": "react-scripts serve-prod",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/main.*",
    "flow": "flow",
    "postinstall": "if test \"$NODE_ENV\" = \"development\" ; then flow-typed install; fi",
    "build-storybook": "build-storybook -s public",
    "styleguide": "start-storybook -p 9001 -c .storybook --ci --quiet"
  },
  "devDependencies": {
    "@storybook/addon-actions": "4.0.7",
    "@storybook/addon-links": "4.0.7",
    "@storybook/addons": "4.0.7",
    "@storybook/react": "4.0.7",
    "axe-core": "3.1.2",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "flightplan": "0.6.19",
    "flow-bin": "0.86.0",
    "flow-typed": "2.5.1",
    "husky": "1.1.2",
    "lint-staged": "8.1.0",
    "memory-fs": "0.4.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.14.3",
    "react-test-renderer": "16.7.0",
    "require-from-string": "2.0.2",
    "source-map-explorer": "1.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "(client|server|shared)/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
